import numpy as np
import matplotlib.pyplot as plt

# Generate random data for X and Y
np.random.seed(42)  # For reproducibility
x = np.random.randn(100)  # Random normal distribution for X
y = np.random.randn(100)  # Random normal distribution for Y

# Create scatter plot with empty circles
plt.scatter(x, y, facecolors='none', edgecolors='blue', alpha=0.7)

# Add labels and title
plt.xlabel('X Values')
plt.ylabel('Y Values')
plt.title('Scatter Plot with Empty Circles')

# Show the grid
plt.grid(True, linestyle='--', alpha=0.6)

# Show the plot
plt.tight_layout()
plt.show()
